// File name: projects/01/Xor.hdl
/**
 * Exclusive-or gate:
 * out = (not(a) and b) or (a and not(b))
 */
CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=sal1);
    Not(in=b, out=sal2);
    And(a=sal1, b=b, out=sal3);
    And(a=a, b=sal2, out=sal4);
    Or(a=sal3, b=sal4, out=out);
}